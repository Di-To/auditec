// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Tree {
  id           String @id @default(cuid())
  region       String
  municipality String
  park         String
  picture      String
  latitute     Float
  longitude    Float
}
